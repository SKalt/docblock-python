{
  "name": "docblock-python",
  "main": "./lib/docblock-python",
  "version": "0.15.1",
  "description": "Inserts documentation blocks for python functions",
  "keywords": [
    "python",
    "documentation",
    "autocomplete",
    "linter"
  ],
  "repository": "https://github.com/spadarian/docblock-python",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "providedServices": {
    "linter": {
      "versions": {
        "2.0.0": "provideLinter"
      }
    }
  },
  "dependencies": {
    "atom-linter": "^10.0.0",
    "atom-package-deps": "^4.0.1"
  },
  "devDependencies": {
    "@types/atom": "^1.36.1",
    "eslint": "^5.0.1",
    "eslint-config-google": "^0.9.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "prettier": "^1.18.2",
    "rollup": "^1.25.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-typescript": "^1.0.1",
    "tslib": "^1.10.0",
    "typescript": "^3.6.4"
  },
  "package-deps": [
    "linter"
  ],
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint ./spec/",
    "pretest": "npm run lint",
    "test": "atom --test ./spec/"
  }
}
